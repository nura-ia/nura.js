{
  "name": "@nurajs/client",
  "version": "0.2.0",
  "description": "Interact with the Nura AI intent surface from any UI or agent client",
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-ia/nurajs.git",
    "directory": "packages/client"
  },
  "homepage": "https://github.com/nura-ia/nurajs/tree/main/packages/client",
  "bugs": {
    "url": "https://github.com/nura-ia/nurajs/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --treeshake",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run"
  },
  "keywords": [
    "nura",
    "intents",
    "client",
    "sdk",
    "ui"
  ],
  "dependencies": {
    "@nurajs/intents": "workspace:*"
  }
}
